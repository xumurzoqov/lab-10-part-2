#include <iostream>
using namespace std;

int findLargestElement(const int* arr, int size) {
    int largest = *arr;

    for (int i = 1; i < size; i++) {
        if (*(arr + i) > largest) {
            largest = *(arr + i);
        }
    }
    return largest;
}

int main() {
    int n;
    cin >> n;
    int arr[n];
    for (int i = 0; i < n; i++) {
        cin >> arr[i];
    }

    cout << findLargestElement(arr, n) << endl;
    return 0;
}
