#include <iostream>
using namespace std;

int cstrLength(char* c) {
    int length = 0
    ;
    while (*c != '\0') {   
        length++;         
        c++;               
    }
    return length;
}

int main() {
    char str[100];
    cin.getline(str, 100);   
    cout << cstrLength(str) << endl;
    return 0;
}
